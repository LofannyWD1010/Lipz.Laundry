<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.lipzlaundry">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logoo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/logoo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".PesananLaundry"></activity>
        <activity android:name=".PemesananSatuan" />
        <activity android:name=".Pemesanan" />
        <activity android:name=".JadwalPengambilan" />
        <activity android:name=".JadwalPenyerahan" />
        <activity android:name=".Login" />
        <activity android:name=".MainActivity" />
        <activity android:name=".Daftar" />
        <activity
            android:name=".SplashScreen"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>